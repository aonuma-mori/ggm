{% extends 'default_frame.twig' %}

{% set body_class = 'registration_page' %}

{% block main %}
  <div class="ggm-ec-role ec-role">
    <div class="ggm-ec-page-header ec-pageHeader">
      <h1>{{ 'front.entry.complete_title__temporary'|trans }}</h1>
    </div>
  </div>
  <div class="ggm-ec-register-complete-role ec-registerCompleteRole">
    <div class="ggm-ec-off3-grid ec-off3Grid ec-text-ac">
      <div class="ggm-ec-off3-grid-cell ec-off3Grid__cell">
        <div class="ggm-ec-report-heading ec-reportHeading">
          <h2>{{ 'front.entry.complete_message__title'|trans }}</h2>
        </div>
        <p class="ggm-ec-report-description ec-reportDescription">{{ 'front.entry.complete_message__temporary'|trans|nl2br }}</p>
        <div class="ggm-ec-off4-grid ec-off4Grid">
          <div class="ggm-ec-off4-grid-cell ec-off4Grid__cell"><a class="ec-blockBtn--cancel" href="{{ url('homepage') }}">{{ 'common.go_to_top'|trans }}</a></div>
        </div>
      </div>
    </div>
  </div>
{% endblock %}
